From 13cebce66ab04ee86225106c0c54281ddc9d3dc3 Mon Sep 17 00:00:00 2001
From: Solo <anqi.huang@outlook.com>
Date: Thu, 10 Mar 2022 11:22:19 +0800
Subject: [PATCH] disable show update time

Signed-off-by: Solo <anqi.huang@outlook.com>
---
 _config.yml | 14 +++++++-------
 1 file changed, 7 insertions(+), 7 deletions(-)

diff --git a/_config.yml b/_config.yml
index 7cb5fcf..bb19972 100644
--- a/_config.yml
+++ b/_config.yml
@@ -252,8 +252,8 @@ post_meta:
   item_text: true
   created_at: true
   updated_at:
-    enable: true
-    another_day: true
+    enable: false
+    another_day: false
   categories: true
 
 # 字数和阅读时间统计插件
@@ -372,7 +372,7 @@ codeblock:
   copy_button:
     enable: true
     # Show text copy result.
-    show_result: false
+    show_result: true
     # Available values: default | flat | mac
     style: mac
 
@@ -394,7 +394,7 @@ reading_progress:
 
 # Bookmark Support
 bookmark:
-  enable: false
+  enable: true
   # Customize the color of the bookmark.
   color: "#222"
   # If auto, save the reading progress when closing the page or clicking the bookmark-icon.
@@ -423,7 +423,7 @@ github_banner:
 # ---------------------------------------------------------------
 
 font:
-  enable: false
+  enable: true
 
   # Uri of fonts host, e.g. https://fonts.googleapis.com (Default).
   host:
@@ -467,11 +467,11 @@ font:
 # ---------------------------------------------------------------
 
 # Disable Baidu transformation on mobile devices.
-disable_baidu_transformation: false
+disable_baidu_transformation: true
 
 # If true, site-subtitle will be added to index page.
 # Remember to set up your site-subtitle in Hexo `_config.yml` (e.g. subtitle: Subtitle)
-index_with_subtitle: false
+index_with_subtitle: true
 
 # Automatically add external URL with Base64 encrypt & decrypt.
 exturl: false
-- 
2.20.1

